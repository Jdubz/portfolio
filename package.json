{
  "private": true,
  "name": "josh-wentworth-portfolio",
  "description": "Josh Wentworth - Multidisciplinary engineer portfolio featuring software, hardware, and fabrication projects",
  "version": "1.6.0",
  "author": "Josh Wentworth",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "develop:cypress": "cross-env CYPRESS_SUPPORT=y yarn develop",
    "build:cypress": "cross-env CYPRESS_SUPPORT=y yarn build",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "npm run lint:tsc && npm run lint:eslint && npm run lint:prettier",
    "lint:tsc": "tsc --project tsconfig.lint.json",
    "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx --max-warnings 0 src/ gatsby-*.ts",
    "lint:eslint:report": "eslint --ext .js,.jsx,.ts,.tsx src/ gatsby-*.ts",
    "lint:prettier": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" \"*.{js,ts,json,md}\"",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:prettier:fix",
    "lint:eslint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix src/ gatsby-*.ts",
    "lint:prettier:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" \"*.{js,ts,json,md}\"",
    "format": "npm run lint:fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lighthouse": "lhci autorun",
    "lighthouse:view": "lhci open",
    "lighthouse:local": "npm run build && npm run serve & npx wait-on http://localhost:9000 && lighthouse http://localhost:9000 --view --preset=desktop --output=html --output-path=./lighthouse-report.html; kill $!",
    "lighthouse:report": "lighthouse http://localhost:9000 --output=html,json --output-path=./lighthouse-report --preset=desktop",
    "deploy:staging": "npm run build && firebase deploy --only hosting:staging",
    "deploy:production": "npm run build && firebase deploy --only hosting:production",
    "deploy:all": "npm run build && firebase deploy --only hosting",
    "firebase:serve": "firebase serve --only hosting --port 5000",
    "firebase:emulate": "firebase emulators:start --only hosting",
    "firebase:login": "firebase login",
    "firebase:projects": "firebase projects:list",
    "firebase:use": "firebase use static-sites-257923",
    "version:patch": "npm version patch -m \"chore: bump version to %s\"",
    "version:minor": "npm version minor -m \"chore: bump version to %s\"",
    "version:major": "npm version major -m \"chore: bump version to %s\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@lekoarts/gatsby-theme-cara": "^5.1.7",
    "@parcel/watcher": "^2.5.1",
    "@radix-ui/react-slot": "^1.2.3",
    "gatsby": "^5.15.0",
    "gatsby-plugin-image": "^3.15.0",
    "gatsby-plugin-manifest": "^5.15.0",
    "gatsby-plugin-sharp": "^5.15.0",
    "gatsby-plugin-sitemap": "^6.15.0",
    "gatsby-plugin-webpack-statoscope": "^1.0.3",
    "gatsby-transformer-sharp": "^5.15.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/jest-dom": "^6.9.0",
    "@testing-library/react": "^14.3.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.18.7",
    "@types/react": "^18.3.25",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "ajv": "^8.17.1",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.0",
    "firebase-tools": "^13.35.1",
    "gatsby-plugin-postcss": "^6.15.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lighthouse": "^12.8.2",
    "playwright": "^1.55.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14",
    "ts-jest": "^29.4.4",
    "typescript": "^5.8.3",
    "wait-on": "^9.0.1",
    "webpack": "^5.102.0"
  }
}
