{
  "name": "contact-form-function",
  "version": "1.0.0",
  "description": "GCP Cloud Function for handling portfolio contact form submissions",
  "main": "index.js",
  "scripts": {
    "start": "functions-framework --target=handleContactForm --port=8080 --host=0.0.0.0",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsc",
    "dev": "tsc --watch & nodemon --exec 'functions-framework --target=handleContactForm --port=8080 --host=0.0.0.0'"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^3.4.6",
    "@google-cloud/logging": "^11.0.0",
    "@google-cloud/secret-manager": "^5.5.0",
    "cors": "^2.8.5",
    "joi": "^17.13.3",
    "nodemailer": "^6.9.15"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.16.5",
    "@types/nodemailer": "^6.4.17",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "functions-framework": "^0.6.10",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": "20"
  },
  "keywords": [
    "gcp",
    "cloud-functions",
    "contact-form",
    "email",
    "portfolio"
  ]
}
